@model ICollection<AuthenticationClientData>

@if (Model.Count == 0)
{
    <div class="message-info">
    </div>
}
else
{
    using (Html.BeginForm("ExternalLogin", "Account", new {ViewBag.ReturnUrl}))
    {
        <p>
            <span class="left">Вы также можете войти через</span>
            @Html.AntiForgeryToken()
            @foreach (AuthenticationClientData p in Model)
            {
                <button type="submit" name="provider" value="@p.AuthenticationClient.ProviderName" class='social-btn @p.ExtraData["divClass"]' title="Войти через @p.DisplayName">
                </button>
            }
        </p>
    }
    
}
